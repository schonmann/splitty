{
  "name": "splitty",
  "version": "1.0.24",
  "description": "Splitty is a simple static file server with a embedded text editor based on ACE. Splitty is focused to speed up the web development  allowing the programmer to use de ACE editor on the left side and see the result on the right side of the same browser tab.",
  "scripts": {
    "test": "mocha"
  },
  "main":"./index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shortty/splitty.git"
  },
  "keywords": [
    "node",
    "http",
    "static",
    "server",
    "editor",
    "text"
  ],
  "author": "Philippe Moneda philippemoneda@gmail.com",
  "contributors":[{"name":"Felipe Pena", "email": "felipe@php.net"}],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shortty/splitty/issues"
  },
  "homepage": "http://shortty.github.io/splitty/",
  "dependencies": {
    "mocha": "2.3.4",
    "serve-static":"1.10.0",
    "finalhandler":"0.4.1",
    "request":"2.67.0",
    "socket.io":"1.3.7",		
    "shelljs":"0.5.3",
    "crypto-js":"3.1.6",
    "open":"0.0.5"
  },
  "preferGlobal": true,
  "bin": {
    "splitty": "index.js",
    "_editor":"_editor/"
  }
}
